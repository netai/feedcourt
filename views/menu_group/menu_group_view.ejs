      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Menu Group Detail</h4>
      </div>
      <div class="modal-body">
    <% if(menu_group.menu_group_images.length>0 && menu_group.menu_group_images[0].img_name!=='undefined'){ %>      
      <div class="col-lg-12">
          <div class="col-xs-7 modal-image-container">
              <a href="javascript:void(0)" class="thumbnail">
                  <img src="/media/images/thumb/<%=menu_group.menu_group_images[0].img_name%>" class="img-rounded center-block" alt="" style="height:150px">
              </a>
          </div>
        </div>
        <% } %>
        <% if(typeof menu_group!='undefined'){ %>
            <table class="table table-striped table-bordered table-hover">
                <tbody>
                    <tr>
                        <td>Name</td>
                        <td><%=menu_group.name%></td>
                    </tr>
                    <tr>
                        <td>Description</td>
                        <td><%=menu_group.description%></td>
                    </tr>
                </tbody>
            </table>
        <% }else{ %>
         <tbody><tr>No Result Found.</tr></tbody>
        <% } %>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">OK</button>
      </div>