      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Order Detail</h4>
      </div>
      <div class="modal-body">
        <div class="row">
        <div class="col-lg-12">
          <div class="panel panel-default">
            <class="panel-heading">
              <div class=""><strong>Invoice No:</strong><%= orders[0].invoice_no%></div>
              <div class=""><strong>Order Date:</strong><%= orders[0].order_master.order_date%></div>
            </div>
            <table class="table table-striped table-bordered table-hover">
                <tbody>
                    <tr>
                        <td>Customer Info</td>
                        <td>
                          <strong>Name:</strong><%=orders[0].order_master.customer.full_name%>;
                          <strong>Email:</strong><%=orders[0].order_master.customer.email%>;
                          <strong>Phone:</strong><%=orders[0].order_master.customer.phone_no%>
                        </td>
                    </tr>
                    <tr>
                        <td>Shipping Address</td>
                        <td>
                          <strong>City:</strong><%=orders[0].ship_address.city.name%>;
                          <strong>State:</strong><%=orders[0].ship_address.state.name%>;
                          <strong>Zip:</strong><%=orders[0].ship_address.zip_code%>
                        </td>
                    </tr>
                    <tr>
                        <td>Billing Address</td>
                        <td>
                          <strong>City:</strong><%=orders[0].bill_address.city.name%>;
                          <strong>State:</strong><%=orders[0].bill_address.state.name%>;
                          <strong>Zip:</strong><%=orders[0].bill_address.zip_code%>
                        </td>
                    </tr>
                    <tr>
                        <td>Outlet Info</td>
                        <td>
                          <strong>Name:</strong><%=orders[0].restaurant.full_name%>;
                          <strong>Email:</strong><%=orders[0].restaurant.email%>;
                          <strong>Phone No:</strong><%=orders[0].restaurant.phone_no%>;
                          <strong>Contact Person:</strong><%=orders[0].restaurant.contact_person%>
                        </td>
                    </tr>
                    <tr>
                        <td>Menu Info</td>
                        <td>
                          <strong>Name:</strong><%=orders[0].menu.title%>
                        </td>
                    </tr>
                    <tr>
                        <td>Amount Info</td>
                        <td>
                          <strong>Price:</strong><%=orders[0].price%>;
                          <strong>Qty:</strong><%=orders[0].qty%>;
                          <strong>Total:</strong><%=orders[0].price%>*<%=orders[0].qty%>=<%=(orders[0].price)*(orders[0].qty)%>
                        </td>
                    </tr>
                    <tr>
                        <td>Payment Info</td>
                        <td>
                          <% var payment_stat={'0':'Cancel','1':'Success','2':'Pending'} %>
                          <strong>Payment Mode:</strong><%=orders[0].order_master.payment_master.payment_type%>;
                          <strong>Payment Status:</strong><%=payment_stat[orders[0].payment_detail[0].status]%>;
                        </td>
                    </tr>
                    <tr>
                        <td>Order Status</td>
                        <td>
                          <% var order_stat={'0':'Cancel','1':'Delevered','2':'Procesing','3':'Not Approved'} %>
                          <strong><%=order_stat[orders[0].order_status]%></strong>
                        </td>
                    </tr>
                    
                </tbody>
            </table>
            </div>
            </div>
            </div>
      </div>
      <!--<div class="modal-footer">-->
      <!--  <button type="button" class="btn btn-default" data-dismiss="modal">OK</button>-->
      <!--</div>-->