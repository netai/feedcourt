
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Menu Detail</h4>
      </div>
      <div class="modal-body">
        <table class="table table-striped table-bordered table-hover">
            <tbody>
                <% if(typeof menues!=='undefined'){ %>
                <tr>
                    <td>Menu Name</td>
                    <td><%=menues.title%></td>
                </tr>
                <tr>
                    <td>Cuisine</td>
                    <td><%=menues.cuisines.title%></td>
                </tr>
                <tr>
                    <td>Price</td>
                    <td><%=menues.price%></td>
                </tr>
                <tr>
                    <td>Unit</td>
                    <td><%if(typeof menues.unites.title!=undefined){%><%=menues.unites.title%><%}%></td>
                </tr>
                <tr>
                    <td>Description</td>
                    <td><%=menues.description%></td>
                </tr>
                
                <% } else { %>
                <tr>
                    No Record Found
                </tr>
                <% } %>
            </tbody>
        </table>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">OK</button>
      </div>