      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Foodcourt Detail</h4>
      </div>
      <div class="modal-body">
    <% if(image!=""){ %>      
      <div class="col-lg-12">
          <div class="col-xs-7 modal-image-container">
              <a href="javascript:void(0)" class="thumbnail">
                  <img src="/media/images/thumb/<%=image.img_name%>" class="img-rounded center-block" alt="Cinque Terre" style="height:150px">
              </a>
          </div>
        </div>
        <% } %>
        <% if(typeof foodcourt!='undefined'){ %>
            <table class="table table-striped table-bordered table-hover">
                <tbody>
                    <tr>
                        <td>Name</td>
                        <td><%=foodcourt.full_name%></td>
                    </tr>
                    <tr>
                        <td>Email</td>
                        <td><%=foodcourt.email%></td>
                    </tr>
                    <tr>
                        <td>Phone-no</td>
                        <td><%=foodcourt.phone_no%></td>
                    </tr>
                    <tr>
                        <td>State</td>
                        <td><%=foodcourt.addresses.state.name%></td>
                    </tr>
                    <tr>
                        <td>City</td>
                        <td><%=foodcourt.addresses.city.name%></td>
                    </tr>
                </tbody>
            </table>
        <% }else{ %>
         <tbody><tr>No Result Found.</tr></tbody>
        <% } %>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">OK</button>
      </div>