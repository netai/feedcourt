      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Foodcourt Detail</h4>
      </div>
      <div class="modal-body" data-ng-if="customerCtrl.customerDetail">
      <div class="col-lg-12">
          <div class="col-xs-7 modal-image-container">
              <a href="javascript:void(0)" class="thumbnail">
                  <img src="/static/assets/upload/xx.jpg" class="img-rounded center-block" alt="Cinque Terre">
              </a>
          </div>
        </div>
        <% if(dataJsonArr.data!=undefined && dataJsonArr.status!=undefined && dataJsonArr.status=='success'){ %>
            <table class="table table-striped table-bordered table-hover">
                <tbody>
                    <tr>
                        <td>Name</td>
                        <td><%=dataJsonArr.data.full_name%></td>
                    </tr>
                    <tr>
                        <td>Email</td>
                        <td><%=dataJsonArr.data.email%></td>
                    </tr>
                    <tr>
                        <td>Phone-no</td>
                        <td><%=dataJsonArr.data.phone_no%></td>
                    </tr>
                    <tr>
                        <td>State</td>
                        <td><%=dataJsonArr.data.addresses.state.name%></td>
                    </tr>
                    <tr>
                        <td>City</td>
                        <td><%=dataJsonArr.data.addresses.city.name%></td>
                    </tr>
                </tbody>
            </table>
        <% }else{ %>
         <tbody><tr>No Result Found.</tr></tbody>
        <% } %>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">OK</button>
      </div>